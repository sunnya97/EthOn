        <rdfs:comment xml:lang="en">A variant of the Ethereum protocol. Changes in the protocol result in a hard or soft fork.</rdfs:comment>
        <rdfs:label xml:lang="en">Ethereum Protocol Variant</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>



    <!-- http://ethon.consensys.net/ReceiptsTrie -->

    <owl:Class rdf:about="http://ethon.consensys.net/ReceiptsTrie">

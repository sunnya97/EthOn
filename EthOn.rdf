        <rdfs:range rdf:resource="http://ethon.consensys.net/TxReceipt"/>
        <suggestedStringRepresentation>hasTransactionReceipt</suggestedStringRepresentation>
        <rdfs:comment xml:lang="en">Relates a transaction to its receipt.</rdfs:comment>
        <rdfs:label xml:lang="en">has transaction receipt</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>



    <!-- http://ethon.consensys.net/hasReceiptsTrie -->

    <owl:ObjectProperty rdf:about="http://ethon.consensys.net/hasReceiptsTrie">
        <rdfs:subPropertyOf rdf:resource="http://ethon.consensys.net/BlockObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <suggestedStringRepresentation>hasReceiptsTrie</suggestedStringRepresentation>
        <rdfs:comment xml:lang="en">Relates a Block to the Trie that contains the Block&apos;s receipt data.</rdfs:comment>
